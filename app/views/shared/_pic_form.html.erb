<%= form_for @pic, :html => { :multipart => true } do |f| %>
	<%= render 'shared/error_messages', object: f.object %>
	<div class="field">
		
		<%= f.label :title %>
		<%= f.text_field :title %>

		<%= f.label :description %>
		<%= f.text_field :description %>

		<%= f.label :pic_file %><br />
		<%= f.file_field :pic_file %>
	</div>
	<%= f.submit "upload image", class: "btn btn-large btn-primary" %>
<% end %>